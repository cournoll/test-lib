{
  "name": "web-cloud-frontend-commons",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "source": "src/index.js",
  "files": [
    "dist",
    "scripts"
  ],
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react-scripts": "4.0.1",
    "rollup-plugin-generate-package-json": "^3.2.0"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:lib && npm run build:docs",
    "build:clean": "rimraf dist && rimraf docs",
    "build:docs": "documentation build src/** -f html -o docs",
    "build:lib": "rollup -c",
    "start": "npm run build:clean && npm run build:docs && rollup -c -w",
    "start-playground": "cd playground && npm start",
    "i-playground": "cd playground && npm i",
    "postinstall": "./scripts/postinstall.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@rollup/plugin-babel": "^5.2.2",
    "documentation": "^13.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build; git add dist/* docs/*"
    }
  }
}
