{
  "name": "web-cloud-frontend-commons",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "source": "src/index.js",
  "files": [
    "dist",
    "scripts"
  ],
  "dependencies": {
    "react-scripts": "4.0.1"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:lib && npm run build:docs",
    "build:clean": "rimraf dist && rimraf docs",
    "build:docs": "jsdoc -d docs -r ./README.md src/**",
    "build:lib": "rollup -c",
    "build:example": "rimraf web-cloud-frontend-commons-0.1.0.tgz && npm pack && cd example && npm rm web-cloud-frontend-commons && npm i ../web-cloud-frontend-commons-0.1.0.tgz",
    "start": "rollup -c -w",
    "start:example": "npm run build:example && cd example && npm start",
    "install:example": "cd example && npm i",
    "postinstall": "./scripts/postinstall.sh",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@rollup/plugin-babel": "^5.2.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "jsdoc": "^3.6.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm t; npm run build; git add dist/* docs/*"
    }
  }
}
